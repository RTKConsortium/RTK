__all__ = [
    "version"
]

RTK_VERSION_MAJOR = "@RTK_VERSION_MAJOR@"
RTK_VERSION_MINOR = "@RTK_VERSION_MINOR@"
RTK_VERSION_PATCH = "@RTK_VERSION_PATCH@"
RTK_VERSION_HASH = "@RTK_VERSION_HASH@"

def version():
    return f"RTK {RTK_VERSION_MAJOR}.{RTK_VERSION_MINOR}.{RTK_VERSION_PATCH}{RTK_VERSION_HASH}"